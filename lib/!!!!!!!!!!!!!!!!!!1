class GameLogic
  @initial_array = nil

  def initialize()
    @initial_array = [[1, 1, 1],
                      [4, 5, 6],
                      [7, 8, 9]]
  end

  def test_winner()
    @initial_array.each do |sub_array|
      return true if sub_array.all?(sub_array[0]) == sub_array[0]
    end
    false
  end

end


p GameLogic.new.test_winner

